{
  "name": "@bonsai/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "npx tsx scripts/populate-questions.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "websocket:start": "npx tsx scripts/start-websocket.ts",
    "websocket:dev": "npx tsx --watch scripts/start-websocket.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run websocket:dev\"",
    "start:full": "concurrently \"npm run start\" \"npm run websocket:start\""
  },
  "dependencies": {
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@next/font": "^14.1.0",
    "@bonsai/shared": "workspace:*",
    "@bonsai/database": "workspace:*",
    "@supabase/supabase-js": "^2.39.7",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/ssr": "^0.1.0",
    "next-auth": "^4.24.6",
    "@tanstack/react-query": "^5.25.0",
    "zustand": "^4.5.1",
    "framer-motion": "^11.0.8",
    "lucide-react": "^0.344.0",
    "date-fns": "^3.3.1",
    "zod": "^3.22.4",
    "react-hook-form": "^7.50.1",
    "@hookform/resolvers": "^3.3.4",
    "stripe": "^14.21.0",
    "@stripe/stripe-js": "^3.0.6",
    "resend": "^3.2.0",
    "sonner": "^1.4.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "recharts": "^2.12.2",
    "openai": "^4.24.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
